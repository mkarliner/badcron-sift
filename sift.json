{
  "name": "crontest",
  "display-name": "Crontest Sift",
  "description": "",
  "author": "",
  "version": "1.0.0",
  "icon": "assets/blueprint.svg",
  "interfaces": {
    "email": {
      "thread": {
        "controller": "dist/js/controller.umd-es2015.min.js"
      },
      "client": {
        "controller": "dist/js/email-client-controller.umd-es2015.min.js"
      }
    },
    "summary": {
      "controller": "dist/js/controller.umd-es2015.min.js"
    }
  },
  "dag": {
    "inputs": {
      "emails": {},
      "slack-bot": {},
      "webhooks": {},
      "rpc": {}
    },
    "nodes": [{
      "#": "First Node",
      "implementation": {
        "javascript": "server/node1.js",
        "when": {
          "crontab": "0 * * * 1-5",
          "run": {
            "install": true,
            "update": false
          }
        }
      },
      "input": {
        "bucket": "settings"
      },
      "outputs": {}
    }],
    "stores": {
      "settings": {
        "key$schema": "string"
      }
    },
    "outputs": {}
  }
}
